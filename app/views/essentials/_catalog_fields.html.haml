.form-inline.clearfix
	.row
		.nested-fields
			= f.input :song_name,  placeholder: "Add Catalog Song", label: false, input_html: { class: 'form-input form-control'}
			= f.input :catalog_url,  placeholder: "URL", label: false, input_html: { class: 'form-input form-control'}
			= f.input :catalog_url_type, collection: ["Youtube", "Spotify", "SoundCloud", "Apple Music", "Google Play"], input_html: { class: "form-control form-input" }
			= link_to_remove_association "Remove", f, class: 'btn btn-secondary form button'
